CC = gcc
ouput = ThreadBridge

main:
		$(CC) -std=gnu99 -o $(ouput) main.c arduino-serial-lib.c arduinoComunication.c Thread.c Scheduler.c TCB.c ContextManage.c Dispatcher.c Mutex.c TCB_list.c TCB_Queue.c BridgeController.c Brigde.c Car.c Car_List.c Car_Queue.c readFile.c CarQueueController.c Interfaz.c  `sdl-config --cflags --libs` -Wall -lm



clean:
		rm $(ouput)




